{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "noImplicitAny": true,
    "paths": {
      "~/*": ["./src/*"]
    },
    "lib": [
      "ESNext"
    ],
    "baseUrl": ".",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "types": [
        "@cloudflare/workers-types/2023-07-01",
        "node"
    ],
    "outDir": "./.dist"
  }
}
